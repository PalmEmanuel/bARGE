{
  "name": "Azure Resource Graph KQL",
  "scopeName": "source.kql.arg",
  "fileTypes": [
    "kql"
  ],
  "patterns": [
    {
      "include": "#comments"
    },
    {
      "include": "#keywords"
    },
    {
      "include": "#operators"
    },
    {
      "include": "#functions"
    },
    {
      "include": "#tables"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#numbers"
    },
    {
      "include": "#properties"
    }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.kql",
          "match": "//.*$"
        },
        {
          "name": "comment.block.kql",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.kql",
          "match": "(?i)\\b(count|distinct|extend|join|innerunique|inner|leftouter|limit|mvexpand|mv-expand|order|parse|project|project-away|sort|summarize|take|top|union|where|contains|!contains|has|!has|hasprefix|!hasprefix|hassuffix|!hassuffix|startswith|!startswith|endswith|!endswith|matches|regex|in|!in)\\b"
        },
        {
          "name": "keyword.operator.logical.kql",
          "match": "(?i)\\b(and|or|not)\\b"
        },
        {
          "name": "keyword.operator.comparison.kql",
          "match": "(==|!=|<>|<=|>=|<|>|=~|!~|contains|!contains|has|!has|hasprefix|!hasprefix|hassuffix|!hassuffix|in|!in|startswith|!startswith|endswith|!endswith|matches|regex)"
        },
        {
          "name": "keyword.other.kql",
          "match": "(?i)\\b(by|asc|desc|nulls|first|last|with|on|kind|inner|outer|left|right|semi|anti|fullouter|innerunique|leftouter|rightouter|leftanti|rightanti|leftsemi|rightsemi)\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.pipe.kql",
          "match": "\\|"
        },
        {
          "name": "keyword.operator.assignment.kql",
          "match": "="
        },
        {
          "name": "keyword.operator.arithmetic.kql",
          "match": "[+\\-*/%]"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "support.function.builtin.kql",
          "match": "(?i)\\b(join|isnotnull|count|tostring|aggregation)\\b"
        }
      ]
    },
    "tables": {
      "patterns": [
        {
          "name": "support.class.table.kql",
          "match": "(?i)\\b(advisorresources|aksresources|alertsmanagementresources|appserviceresources|authorizationresources|awsresources|azurebusinesscontinuityresources|azuredevopsplatformresources|batchresources|capabilityresources|chaosresources|communitygalleryresources|computeresources|containerserviceeventresources|deploymentresources|desktopvirtualizationresources|dnsresources|edgeorderresources|elasticsanresources|extendedlocationresources|extensibilityresourcechanges|featureresources|gcpresources|guestconfigurationresources|healthresourcechanges|healthresources|impactreportresources|insightsresources|iotsecurityresources|kubernetesconfigurationresources|kustoresources|machinesinventoryinsightsresources|maintenanceresourcechanges|maintenanceresources|managedservicesresources|migrateresources|networkresourcechanges|networkresources|orbitalresources|patchassessmentresources|patchinstallationresources|policyresources|quotaresourcechanges|recoveryservicesresources|resourcechanges|resourcecontainerchanges|resourcecontainers|resources|securityresources|servicefabricresources|servicehealthresources|sharedgalleryresources|spotresources|tagsresources)\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.kql",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.kql",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.kql",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.kql",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.kql",
          "match": "\\b([0-9]+\\.?[0-9]*([eE][+-]?[0-9]+)?[fdm]?)\\b"
        },
        {
          "name": "constant.numeric.hex.kql",
          "match": "\\b0[xX][0-9a-fA-F]+\\b"
        }
      ]
    },
    "properties": {
      "patterns": [
        {
          "name": "variable.other.property.kql",
          "match": "(?i)\\b(id|name|type|kind|location|resourceGroup|subscriptionId|tenantId|managedBy|sku|plan|properties|tags|identity|zones|extendedLocation|score|state|typeVersion|workloadName|assessmentId|description|typeId|locale|workloadId|exclude|lowCpuThreshold|duration|recommendationTypeId|category|sourceSystem|impactedField|impact|impactedValue|lastUpdated|potentialBenefits|learnMoreLink|notes|tracked|label|correlationId|recommendationsCount|reviewStatus|publishedAt|reviewName|updatedAt|expirationTimeStamp|suppressionId|ttl|title|reviewId|recommendationStatus|rejectReason|priority|resourceId|recommendationId|resourceName|resourceType|hubAccessMode|certificateAuthorityData|provisioningState|fleetResourceId|fleetHubUrl|memberName|disabled|channel|longTermSupport|updateStrategyId|targetKubernetesVersion|displayName|gateType|clusterResourceId|group|autoUpgradeProfileId|aadTenantId|isInternal|geography|provisioningStateTransitionTime|allocationStateTransitionTime|currentLowPriorityNodes|interNodeCommunication|currentDedicatedNodes|taskSlotsPerNode|allocationState|vmSize|creationTime|lastModified|targetNodeCommunicationMode|currentNodeCommunicationMode|timestamp|isDeleted|status|stoppedAt|startedAt|allowPublicAccess|agentProfileId|privateAccessId|agentTenantId|parametersSchema|urn|targetType|publisher|latestModelApplied|timeCreated|modelDefinitionApplied|vmId|evictionPolicy|licenseType|userData|enableAcceleratedNetworking|disableTcpStateTracking|resourceGuid|enableIPForwarding|vnetEncryptionSupported|primary|allowPort25Out|enableFpga|auxiliaryMode|defaultOutboundAccess|auxiliarySku|macAddress|nicType|deploymentId|deploymentScope|eTag|OrderMode|protocolType|encryption|volumeId|sizeGiB|clusterExtensionId|extensionType|shouldFeatureDisplayInPortal|providerNamespace|approvalType|featureName|documentationLink|releaseDate|groupShortName|enabled|version|vendor|model|fileName|fileSize|zone|tiAutomaticUpdates|connectivityTime|sensorType|dynamicLearning|learningMode|sens